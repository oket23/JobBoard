{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "your_connection_string",
    "Redis": "your_connection_string",
    "RabbitMQ": "your_connection_string"
  },
  "JwtSettings": {
    "SecretKey": "your_key",
    "Issuer": "your-issuer",
    "Audience": "your-clients",
    "AccessExpiryMinutes": -1,
    "RefreshExpiryDays": -1
  }
}
