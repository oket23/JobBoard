{
  "Serilog": {
    "SeqServerUrl": "http://jobboard-seq:5341",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "Microsoft.AspNetCore.Authentication": "Information"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "your_connection_string",
    "Redis": "your_connection_string",
    "RabbitMQ": "your_connection_string"
  },
  "JwtSettings": {
    "SecretKey": "your_secret_key",
    "Issuer": "your_issuer",
    "Audience": "your_adience",
    "AccessExpiryMinutes": -1,
    "RefreshExpiryDays": -1
  },
  "AllowedHosts": "*"
}